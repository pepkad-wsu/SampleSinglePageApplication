<script src="~/js/viewModels/autos/tenantAutos.js?v=@Html.Raw(Guid.NewGuid().ToString().Replace("-", ""))"></script>
<div id="view-tenants-auto">
	<div data-bind="visible:MainModel().CurrentView() == 'tenantsauto'">
		<div data-bind="css:{fixed: MainModel().StickyMenus() == true}">
			<i class="sticky-menu-icon" data-bind="html:MainModel().StickyIcon(), click:function() {MainModel().ToggleStickyMenus()}"></i>
			<h1 class="display-7"><i data-bind="html:MainModel().IconAndText('Tenants')"></i></h1>
			<div class="padbottom-5">
				<div class="btn-group" role="group">
					<button type="button" class="btn btn-success" data-bind="click:function() { MainModel().Nav('newtenantauto') }">
						<i data-bind="html:MainModel().IconAndText('Tenant.AddNew')"></i>
					</button>
					
					<button type="button" class="btn btn-primary" data-bind="click:ClearTenantFilter">
						<i data-bind="html:MainModel().IconAndText('Tenant.Clear')"></i>
					</button>
					
					<button type="button" class="btn btn-dark" data-bind="visible:Filter().showFilters() == false, click:ToggleShowTenantFilter, enable:Loading() == false">
						<i data-bind="html:MainModel().IconAndText('Tenant.ShowFilter')"></i>
					</button>
					<button type="button" class="btn btn-dark" data-bind="visible:Filter().showFilters() == true, click:ToggleShowTenantFilter, enable:Loading()  == false">
						<i data-bind="html:MainModel().IconAndText('Tenant.HideFilter')"></i>
					</button>
					<button type="button" class="btn btn-dark" data-bind="click:RefreshTenantFilter, enable:Loading()  == false">
						<i data-bind="html:MainModel().IconAndText('Tenant.Refresh')"></i>
					</button>
				</div>
			</div>
		</div>

		<div data-bind="visible:GettingSavedTenantFilterName() == true">
		    <label for="saved-tenantfilter-name" data-bind="html:MainModel().Language('Tenant.SavedFilterName')"></label>
		    <div class="input-group">
		        <button type="button" class="btn btn-outline-dark" data-bind="html:MainModel().IconAndText('Cancel'), click:function(){GettingSavedTenantFilterName(false)}"></button>
		        <button type="button" class="btn btn-outline-success" data-bind="html:MainModel().IconAndText('Save'), click:SaveTenantFilterRecord"></button>
		        <input type="text" class="form-control" id="saved-tenantfilter-name" data-bind="value:SavedTenantFilterName" />
		    </div>
		</div>
		<div class="row" data-bind="visible:Loading() == true">
		 <div class="col-sm-12" data-bind="html:MainModel().Language('Tenant.Loading');"></div>
		</div>
		<div class="row" data-bind="visible:Loading() == false">
		 <div data-bind="visible:Filter().showFilters() == true">
		  <div class="row padbottom">
				@* String Name *@
					<div class="col-sm-3 padbottom" data-bind="visible:Filter().showFilters() == true && Loading() == false">
						<label for="tenants-filter-name" data-bind="html:MainModel().Language('Tenant.Name')">Name</label>
						<div class="form-check form-switch right">
							<input type="checkbox" id="tenants-filter-namefilterexact" role="switch" class="form-check-input"  data-bind="checked:Filter().nameFilterExact">
							<label for="tenants-filter-namefilterexact" class="unbold" data-bind="html:MainModel().Language('Tenant.NameFilterExact')">Filter Exact</label>
						</div>
						<input type="text" id="tenants-filter-name" class="form-control" data-bind="value:Filter().name" placeholder="Search Name" />
					</div>
				@* String TenantCode *@
					<div class="col-sm-3 padbottom" data-bind="visible:Filter().showFilters() == true && Loading() == false">
						<label for="tenants-filter-tenantcode" data-bind="html:MainModel().Language('Tenant.TenantCode')">Tenant Code</label>
						<div class="form-check form-switch right">
							<input type="checkbox" id="tenants-filter-tenantcodefilterexact" role="switch" class="form-check-input"  data-bind="checked:Filter().tenantCodeFilterExact">
							<label for="tenants-filter-tenantcodefilterexact" class="unbold" data-bind="html:MainModel().Language('Tenant.TenantCodeFilterExact')">Filter Exact</label>
						</div>
						<input type="text" id="tenants-filter-tenantcode" class="form-control" data-bind="value:Filter().tenantCode" placeholder="Search Tenant Code" />
					</div>
				@* Boolean Enabled *@
					<div class="col-sm-3" data-bind="visible:Filter().showFilters() == true && Loading() == false">
						<label for="tenants-filter-enabled" data-bind="html:MainModel().Language('Tenant.Enabled')"></label>
						<select class="form-control" size="4" id="tenants-filter-enabled" data-bind="value:Filter().enabled">
							<option value="">All</option>
							<option value="true">Is Enabled</option>
							<option value="false">Is not Enabled </option>
						</select>
					</div>
				@* Guid TenantId *@
					<!-- ERROR_ISGUID   TenantId   -->
				@* BooleanResponse ActionResponse *@
					<!-- !ERROR_IsOnEFModel   ActionResponse   -->
				@* DepartmentGroup DepartmentGroups *@
					<!-- !ERROR_IsOnEFModel   DepartmentGroups   -->
				@* Department Departments *@
					<!-- !ERROR_IsOnEFModel   Departments   -->
				@* ListItem ListItems *@
					<!-- !ERROR_IsOnEFModel   ListItems   -->
				@* TenantSettings TenantSettings *@
					<!-- !ERROR_IsOnEFModel   TenantSettings   -->
				@* udfLabel udfLabels *@
					<!-- !ERROR_IsOnEFModel   udfLabels   -->
				@* UDF 1 *@
					<div class="col-3 padbottom" data-bind="visible:MainModel().UDFShowInFilter('Tenants', 1) == true">
						<label for="tenants-filter-udf01" data-bind="text:MainModel().UDFLabel('Tenants', 1)"></label>
						<select class="form-control" id="tenants-filter-udf01" size="4"data-bind="value:Filter().udf01,options: MainModel().UDFFilterOptions('Tenants', 1),optionsCaption: '',valueAllowUnset: true"></select>
					</div>
				@* UDF 2 *@
					<div class="col-3 padbottom" data-bind="visible:MainModel().UDFShowInFilter('Tenants', 2) == true">
						<label for="tenants-filter-udf02" data-bind="text:MainModel().UDFLabel('Tenants', 2)"></label>
						<select class="form-control" id="tenants-filter-udf02" size="4"data-bind="value:Filter().udf02,options: MainModel().UDFFilterOptions('Tenants', 2),optionsCaption: '',valueAllowUnset: true"></select>
					</div>
				@* UDF 3 *@
					<div class="col-3 padbottom" data-bind="visible:MainModel().UDFShowInFilter('Tenants', 3) == true">
						<label for="tenants-filter-udf03" data-bind="text:MainModel().UDFLabel('Tenants', 3)"></label>
						<select class="form-control" id="tenants-filter-udf03" size="4"data-bind="value:Filter().udf03,options: MainModel().UDFFilterOptions('Tenants', 3),optionsCaption: '',valueAllowUnset: true"></select>
					</div>
				@* UDF 4 *@
					<div class="col-3 padbottom" data-bind="visible:MainModel().UDFShowInFilter('Tenants', 4) == true">
						<label for="tenants-filter-udf04" data-bind="text:MainModel().UDFLabel('Tenants', 4)"></label>
						<select class="form-control" id="tenants-filter-udf04" size="4"data-bind="value:Filter().udf04,options: MainModel().UDFFilterOptions('Tenants', 4),optionsCaption: '',valueAllowUnset: true"></select>
					</div>
				@* UDF 5 *@
					<div class="col-3 padbottom" data-bind="visible:MainModel().UDFShowInFilter('Tenants', 5) == true">
						<label for="tenants-filter-udf05" data-bind="text:MainModel().UDFLabel('Tenants', 5)"></label>
						<select class="form-control" id="tenants-filter-udf05" size="4"data-bind="value:Filter().udf05,options: MainModel().UDFFilterOptions('Tenants', 5),optionsCaption: '',valueAllowUnset: true"></select>
					</div>
				@* UDF 6 *@
					<div class="col-3 padbottom" data-bind="visible:MainModel().UDFShowInFilter('Tenants', 6) == true">
						<label for="tenants-filter-udf06" data-bind="text:MainModel().UDFLabel('Tenants', 6)"></label>
						<select class="form-control" id="tenants-filter-udf06" size="4"data-bind="value:Filter().udf06,options: MainModel().UDFFilterOptions('Tenants', 6),optionsCaption: '',valueAllowUnset: true"></select>
					</div>
				@* UDF 7 *@
					<div class="col-3 padbottom" data-bind="visible:MainModel().UDFShowInFilter('Tenants', 7) == true">
						<label for="tenants-filter-udf07" data-bind="text:MainModel().UDFLabel('Tenants', 7)"></label>
						<select class="form-control" id="tenants-filter-udf07" size="4"data-bind="value:Filter().udf07,options: MainModel().UDFFilterOptions('Tenants', 7),optionsCaption: '',valueAllowUnset: true"></select>
					</div>
				@* UDF 8 *@
					<div class="col-3 padbottom" data-bind="visible:MainModel().UDFShowInFilter('Tenants', 8) == true">
						<label for="tenants-filter-udf08" data-bind="text:MainModel().UDFLabel('Tenants', 8)"></label>
						<select class="form-control" id="tenants-filter-udf08" size="4"data-bind="value:Filter().udf08,options: MainModel().UDFFilterOptions('Tenants', 8),optionsCaption: '',valueAllowUnset: true"></select>
					</div>
				@* UDF 9 *@
					<div class="col-3 padbottom" data-bind="visible:MainModel().UDFShowInFilter('Tenants', 9) == true">
						<label for="tenants-filter-udf09" data-bind="text:MainModel().UDFLabel('Tenants', 9)"></label>
						<select class="form-control" id="tenants-filter-udf09" size="4"data-bind="value:Filter().udf09,options: MainModel().UDFFilterOptions('Tenants', 9),optionsCaption: '',valueAllowUnset: true"></select>
					</div>
				@* UDF 10 *@
					<div class="col-3 padbottom" data-bind="visible:MainModel().UDFShowInFilter('Tenants', 10) == true">
						<label for="tenants-filter-udf10" data-bind="text:MainModel().UDFLabel('Tenants', 10)"></label>
						<select class="form-control" id="tenants-filter-udf10" size="4"data-bind="value:Filter().udf10,options: MainModel().UDFFilterOptions('Tenants', 10),optionsCaption: '',valueAllowUnset: true"></select>
					</div>
				</div>
			</div>
		
			<div data-bind="visible:Filter().recordCount() == 0, html:MainModel().Language('Tenant.TenantNoRecords')"></div>
		
			<div data-bind="visible:Filter().recordCount() > 0">
				<div id="tenant-records-auto"></div>
			</div>
		</div>
	</div>

	<div data-bind="visible:(MainModel().CurrentView() == 'edittenantauto' || MainModel().CurrentView() == 'newtenantauto')">
		<div data-bind="css:{fixed: MainModel().StickyMenus() == true}">
			<i class="sticky-menu-icon" data-bind="html:MainModel().StickyIcon(), click:function() {MainModel().ToggleStickyMenus()}"></i>
			<h1 class="display-7">
				<!-- ko if: MainModel().CurrentView() == 'edittenantauto' -->
				<i data-bind="html:MainModel().IconAndText('Tenant.Edit')"></i>
				<span class="element-id" data-bind="visible:Loading() == false">&ldquo;<span data-bind="text:MainModel().Id()"></span>&rdquo;</span>
				<!-- /ko -->
				<!-- ko if: MainModel().CurrentView() == 'newtenantauto' -->
				<i data-bind="html:MainModel().Icon('Tenants')"></i><span class="icon-text" data-bind="html:MainModel().Language('Tenant.AddNew')"></span>
				<span class="element-id" data-bind="visible:Tenant().tenantId() != null && Tenant().tenantId() != MainModel().GuidEmpty()">
					&ldquo;<span data-bind="text:Tenant().tenantId"></span>&rdquo;
				</span>
				<!-- /ko -->
			</h1>
	
			<div class="padbottom">
				<button type="button" class="btn btn-dark" data-bind="click:function() { MainModel().Nav('tenantsauto') }, html:MainModel().IconAndText('Tenant.Back')"></button>
				<button type="button" class="btn btn-success" data-bind="click:SaveTenant, html:MainModel().IconAndText('Tenant.Save')"></button>
				<!-- ko if: MainModel().Id() != null && MainModel().Id() != '' -->
				<!-- ko if: ConfirmDeleteTenant() != MainModel().Id() -->
				<button type="button" class="btn btn-danger" data-bind="html:MainModel().IconAndText('Tenant.ConfirmDelete'), click:function() { ConfirmDeleteTenant(MainModel().Id()) }"></button>
				<!-- /ko -->
				<!-- ko if: ConfirmDeleteTenant() == MainModel().Id() -->
				<div class="btn-group" role="group">
					<button type="button" class="btn btn-dark" data-bind="html:MainModel().IconAndText('Tenant.Cancel'), click:function() { ConfirmDeleteTenant('') }"></button>
					<button type="button" class="btn btn-danger" data-bind="html:MainModel().IconAndText('Tenant.ConfirmDelete'), click:DeleteTenant"></button>
				</div>
				<!-- /ko -->
				<!-- /ko -->
			</div>
		</div>
	
		<div data-bind="visible:Loading() == true, html:MainModel().Language('Tenant.Loading')"></div>
	
		<div data-bind="visible:Tenant().tenantId() != null && Loading() == false">
	
			<table class="table first-column-small first-column-bold first-column-right">
				@* String Name *@
				<tr>
					<td><label for="edit-tenant-name" data-bind="html:MainModel().Language('Tenant.Name', null, true)"></label></td>
					<td><input type="text" id="edit-tenant-name"class="form-control" data-bind="value:Tenant().name" /></td>
				</tr>
				@* String TenantCode *@
				<tr>
					<td><label for="edit-tenant-tenantcode" data-bind="html:MainModel().Language('Tenant.TenantCode', null, true)"></label></td>
					<td><input type="text" id="edit-tenant-tenantcode"class="form-control" data-bind="value:Tenant().tenantCode" /></td>
				</tr>
				@* Boolean Enabled *@
				<tr>
					<td><label for="edit-tenant-enabled" data-bind="html:MainModel().Language('Tenant.Enabled', null, true)"></label></td>
					<td><input type="checkbox" id="edit-tenant-enabled" role="switch" class="form-check-input" data-bind="checked:Tenant().enabled" /></td>
				</tr>
				@* Guid TenantId *@
				@* BooleanResponse ActionResponse *@
				@* DepartmentGroup DepartmentGroups *@
				@* Department Departments *@
				@* ListItem ListItems *@
				@* TenantSettings TenantSettings *@
@*ERROR: // not an int, bool, guid or string, or datetime. but it still on the ef model, how odd, maybe like a double? 			TenantSettings = item.TenantSettings ?? "", //OTHER nullable on ef model: base(TenantSettings) : actual(TenantSettings)*@
				@* udfLabel udfLabels *@
			</table>
		</div>
	
		<div id="edit-tenant-udf-fields" data-bind="visible:Loading() == false"></div>
	</div>
	<form id="tenant-filter-download" method="post" action="/File/DownloadTenants/" target="_blank" style="display:none;">
		<textarea name="tenant-filter" id="tenant-filter" aria-hidden="true"></textarea>
		<input type="hidden" name="tenant-filter-tenantid" id="tenant-filter-tenantid" data-bind="value:MainModel().TenantId()" />
		<input type="hidden" name="tenant-filter-user-token" id="tenant-filter-user-token" data-bind="value:MainModel().Token()" />
	</form>
</div>
